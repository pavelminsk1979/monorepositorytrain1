      "sourceRoot": "apps/monorepositorytrain1/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monorepositorytrain1/tsconfig.app.json"
      }
    },
    "newname": {
      "type": "application",
      "root": "apps/newname",
      "entryFile": "main",
      "sourceRoot": "apps/newname/src",
      "compilerOptions": {
        "tsConfigPath": "apps/newname/tsconfig.app.json"
      }
    }
  }
}